{
  "name": "@osjs/windows8-theme",
  "version": "4.0.5",
  "description": "OS.js v3 Windows 8 Style Theme",
  "files": [
    "dist/",
    "metadata.json"
  ],
  "scripts": {
    "test": "npm run eslint && npm run stylelint",
    "eslint": "eslint index.js",
    "stylelint": "stylelint index.scss",
    "build": "webpack",
    "watch": "webpack --watch",
    "prepublishOnly": "npm run test && rm -rf ./dist/* && NODE_ENV=production npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/os-js/osjs-windows8-theme.git"
  },
  "keywords": [
    "osjs"
  ],
  "dependencies": {},
  "devDependencies": {
    "@osjs/dev-meta": "^1.0.2"
  },
  "author": "Anders Evenrud <andersevenrud@gmail.com>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/os-js/osjs-windows8-theme/issues"
  },
  "homepage": "https://github.com/os-js/osjs-windows8-theme#readme",
  "osjs": {
    "type": "package"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@osjs/eslint-config"
  },
  "stylelint": {
    "extends": "@osjs/stylelint-config"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
